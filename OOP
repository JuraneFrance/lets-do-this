using System;
using System.Collections;
class ProgrArrayListExample
{
static void Main()
 {
 ArrayList list = new ArrayList();
 list.Add("Clothes");
 list.Add(27);
 list.Add("Bags");
 list.Add("Shoes");
 list.Add(777);
 for (int i = 0; i < list.Count; i++)
 {
 object value = list[i];
 Console.WriteLine("Index={0}; Value={1}", i, value);
 }
 }
}
